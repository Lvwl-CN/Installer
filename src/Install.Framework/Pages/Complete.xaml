<Page x:Class="Install.Framework.Pages.Complete"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Install.Framework.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Complete">

    <Grid>
        <Grid Margin="30,50">
            <CheckBox Content="{DynamicResource StrRun}" IsChecked="{Binding Run,Mode=TwoWay}"/>
        </Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Bottom">
            <Button Content="{DynamicResource StrComplete}" Command="{Binding CompleteCommand}" Margin="10,0" MinWidth="80"/>
        </StackPanel>
    </Grid>
</Page>
